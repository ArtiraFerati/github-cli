{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "linebreak-style": 0,
        "indent": [2, 4]
    }
}
